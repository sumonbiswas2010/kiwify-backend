    "multer": "1.4.5-lts.1",
    "mysql2": "^3.1.0",
    "nanoid": "^3.3.4",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.8.0",
    "path": "^0.12.7",
    "pg": "^8.8.0",
    "pm2": "^5.2.2",
    "sequelize": "^6.25.8",
    "sequelize-cli": "^6.5.2",
